<?xml version="1.0"?>
<ruleset name="OYM">
	<description>The OYM coding standard.</description>
	<rule ref="PSR12">
		<exclude name="Generic.Files.LineLength"/>
	</rule>

	<file>src/</file>

	<exclude-pattern type="relative-root">*.js</exclude-pattern>
	<exclude-pattern type="relative-root">migrations/</exclude-pattern>
</ruleset>
