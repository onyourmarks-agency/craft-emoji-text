{
    "name": "tde/craft-emoji-text",
    "description": "This plugin bypasses the error \"X cannot contain emoji\".",
    "type": "craft-plugin",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "craft emoji text"
    ],
    "support": {
        "docs": "https://github.com/TdeNL/tde-emoji-text/blob/master/README.md",
        "issues": "https://github.com/TdeNL/tde-emoji-text/issues"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "TDE B.V.",
            "homepage": "https://www.tde.nl/en/"
        }
    ],
    "require": {
		"php": "^8.0",
        "craftcms/cms": "^4.0.0"
    },
    "autoload": {
        "psr-4": {
          "tde\\craft\\emojitext\\": "src/"
        }
    },
    "extra": {
        "name": "TDE Emoji text",
        "handle": "craft-emoji-text",
        "hasCpSettings": false,
        "hasCpSection": false,
        "changelogUrl": "https://raw.githubusercontent.com/TdeNL/tde-emoji-text/master/CHANGELOG.md",
        "class": "tde\\craft\\emojitext\\TdeEmojiText"
    },
    "config": {
        "allow-plugins": {
            "yiisoft/yii2-composer": true,
            "craftcms/plugin-installer": true
        }
    }
}
