{
    "name": "oym/craft-emoji-text",
    "description": "This plugin bypasses the error \"X cannot contain emoji\".",
    "type": "craft-plugin",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "craft emoji text"
    ],
    "support": {
        "docs": "https://github.com/onyourmarks-agency/oym-emoji-text/blob/master/README.md",
        "issues": "https://github.com/onyourmarks-agency/oym-emoji-text/issues"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "OYM",
            "homepage": "https://www.onyourmarks.agency/en/"
        }
    ],
    "require": {
		"php": "^8.0",
        "craftcms/cms": "^4.0.0"
    },
    "autoload": {
        "psr-4": {
          "oym\\craft\\emojitext\\": "src/"
        },
				"files": ["src/deprecated.php"]
    },
    "extra": {
        "name": "OYM Emoji text",
        "handle": "craft-emoji-text",
        "hasCpSettings": false,
        "hasCpSection": false,
        "changelogUrl": "https://raw.githubusercontent.com/onyourmarks-agency/oym-emoji-text/master/CHANGELOG.md",
        "class": "oym\\craft\\emojitext\\OymEmojiText"
    },
    "config": {
        "allow-plugins": {
            "yiisoft/yii2-composer": true,
            "craftcms/plugin-installer": true
        }
    }
}
